<?xml version="1.0"?>
<Container version="2">
  <Date>2021-08-20</Date>
  <Name>revisionfxlicensingutility</Name>
  <Repository>camjac251/docker-revisionfx-rvm</Repository>
  <Registry>https://hub.docker.com/r/camjac251/docker-revisionfx-rvm/~/dockerfile/</Registry>
  <Network>host</Network>
  <Privileged>false</Privileged>
  <Overview>[b]RevisionFX RLM server for licensing [/b]</Overview>
  <Category>Productivity: Tools: Status:Stable</Category>
  <TemplateURL>https://raw.githubusercontent.com/camjac251/unraid-repo/master/camjac251/revisionfxlicensingutility.xml</TemplateURL>
  <Icon>https://raw.githubusercontent.com/camjac251/unraid-repo/master/Icon/revisionfxlicensingutility.png</Icon>
  <ExtraParams/>
  <Description></Description>
  <Networking>
    <Mode>host</Mode>
    <Publish/>
  </Networking>
  <Data>
    <Volume>
      <HostDir/>
      <ContainerDir>/usr/local/revision/floatinglicenses/Licenses</ContainerDir>
      <Mode>r</Mode>
    </Volume>
  </Data>
  <Environment/>
  <!-- Volume Mappings -->
  <Config Name="License Folder Path" Target="/usr/local/revision/floatinglicenses/Licenses" Default="" Mode="r" Description="Root path containing the license file.lic.&amp;lt;br&amp;gt;Container path: /usr/local/revision/floatinglicenses/Licenses" Type="Path" Display="always-hide" Required="true" Mask="false"/>
</Container>
